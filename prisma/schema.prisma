// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

datasource db {
  provider = "mysql"
  url      = env("MAF_DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id          Int       @id
  firstName   String
  lastName    String
  avatar      String    @db.Text
}

model Game {
  id          Int       @id
  stage       String
  votingStage String
}
